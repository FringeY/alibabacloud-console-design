import getRoutes from '@alicloud/console-design/lib'
import sidebar from '~/sidebar'
import routeConfig from './route_config';

<% if(global.hasLayout){ %>
import AppLayout from '~/layout';
<% } else { %>
import AppLayout from '@alicloud/console-design-framework'
<% } %>

<% if(global.hasAppConfig){ %>
